<?xml version="1.0" encoding="utf-8"?>
<plugin
xmlns="http://www.phonegap.com/ns/plugins/1.0"
xmlns:android="http://schemas.android.com/apk/res/android"
id="com.btncafe.cordova.sktgeofence"
version="1.0.0">
	<name>
		SKTGeofence Cordova Plugin
	</name>
	<author>
		Hanul
	</author>
	<license>
		MIT
	</license>
	<repo>
		https://github.com/Hanul/SKTGeofence-Cordova-Plugin.git
	</repo>
	<issue>
		https://github.com/Hanul/SKTGeofence-Cordova-Plugin/issues
	</issue>
	<js-module src="www/SKTGeofence.js" name="SKTGeofence">
		<clobbers target="SKTGeofence" />
	</js-module>
	<platform name="android">
		<config-file target="res/xml/config.xml" parent="/*">
			<feature name="sktgeofence">
				<param name="android-package" value="com.btncafe.cordova.sktgeofence" />
			</feature>
		</config-file>
		<config-file target="AndroidManifest.xml" parent="/manifest/application">
			<receiver
			android:name="com.btncafe.cordova.sktgeofence.GeoServiceReceiver"
			android:enabled="true"
			android:exported="true" >
				<intent-filter>
					<action android:name="com.btncafe.cordova.sktgeofence.GEOEVENT" />
				</intent-filter>
			</receiver>
		</config-file>
		<source-file src="src/libs/android-support-v4.jar" target-dir="libs" />
		<source-file src="src/libs/GeoFenceApi.jar" target-dir="libs" />
		<source-file src="src/com/btncafe/cordova/sktgeofence/SKTGeofence.java" target-dir="src/com/btncafe/cordova/sktgeofence" />
		<source-file src="src/com/btncafe/cordova/sktgeofence/SKTGeofenceCordovaPlugin.java" target-dir="src/com/btncafe/cordova/sktgeofence" />
	</platform>
</plugin>
